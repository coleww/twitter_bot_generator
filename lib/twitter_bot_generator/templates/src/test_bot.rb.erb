class <%= class_name %>

  class << self

    def generate
      greetings.sample
    end

  protected

    def greetings
      ['Hello World!', 'Hello Twitter!', 'Hello Ruby!']
    end

  private

    def load_txt_file file_name
      (File.readlines (File.join 'lib', file_name)).map &:strip
    end

  end

end
